{
  "name": "task-timer-management",
  "displayName": "Task Timer Management",
  "description": "Extension to manage the Tasks ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dheariarachman/vscode-timer.git"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.startTimer",
        "title": "TaskTime: Start Timer"
      },
      {
        "command": "extension.stopTimer",
        "title": "TaskTime: Stop Timer"
      },
      {
        "command": "extension.showTracking",
        "title": "TaskTime: Show Current Tracking"
      },
      {
        "command": "extension.showActiveTimers",
        "title": "TaskTime: Show Active Timers"
      },
      {
        "command": "extension.showDailySummary",
        "title": "TaskTime: Show Daily Time Summary"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "mocha": "^10.3.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {},
  "license": "MIT"
}